/**
 * This is a program that calculates the BMI
 * 
 * CS 049  Hanzhuo Gong
 */
public class BodyMassIndex {

	public static void main(String[] args) {
	  
		/* Declared variables */
		 double height, weight, BMI;
		 
		
		/* Declared the number and computation */ 
	     height= 168;
	     weight= 68;		 
	     
	     BMI= (weight/((height/100)*(height/100)));
	     
	    /* Output */
	     System.out.print("The BMI value for a person with height "+ height);
	     System.out.print( " cm and weight " + weight);
	     System.out.print(" kg is "+ BMI);
	}

}
